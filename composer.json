{
  "name": "tourze/wechat-work-app-chat-bundle",
  "description": "企业微信群聊服务",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/collections": "^2.3",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^3.1 || ^4",
    "symfony/console": "^6.4",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "tourze/easy-admin-attribute": "0.0.*",
    "tourze/doctrine-snowflake-bundle": "0.0.*",
    "tourze/doctrine-user-bundle": "0.0.*",
    "tourze/doctrine-timestamp-bundle": "0.0.*",
    "tourze/wechat-work-bundle": "0.0.*",
    "tourze/http-client-bundle": "0.1.*",
    "symfony/yaml": "^6.4 || ^7.1",
    "psr/log": "^3|^2|^1"
  },
  "autoload": {
    "psr-4": {
      "WechatWorkAppChatBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WechatWorkAppChatBundle\\Tests\\": "tests"
    }
